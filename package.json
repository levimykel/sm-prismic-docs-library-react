{
  "name": "sm-prismic-docs-library-react",
  "version": "0.0.0",
  "description": "A Slice Library based on the Prismic developer documentation made with Next.js",
  "author": "Levi Gable",
  "license": "MIT",
  "main": "slices/index.js",
  "scripts": {
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:export": "next export",
    "build": "yarn build-storybook && shx mv storybook-static out",
    "prepublishOnly": "node ./node_modules/sm-commons/scripts/bundle",
    "prismic": "prismic",
